{
  "name": "react-redux-starter-kit",
  "scripts": {
    "build:script": "node_modules/.bin/webpack --config=build/webpack.config.js --progress --verbose -d --display-error-details",
    "watch:script": "node_modules/.bin/webpack --config=build/webpack.config.js --progress -w --verbose -d --display-error-details",
    "prod:script": "node_modules/.bin/webpack --config=build/webpack.config.js --progress -p --verbose -d --display-error-details",
    "build:style": "node-sass src/styles/style.scss -o public/dist",
    "watch:style": "npm run build:style && node-sass -w src/styles/style.scss -o public/dist",
    "post:style": "postcss -u autoprefixer --autoprefixer.browsers '> 5%' -u cssnano -o public/dist/style.css public/dist/style.css",
    "prod:style": "npm run build:style && npm run post:style",
    "start": "echo 'Please run a web-server to serve `public/` directory' && npm-run-all --parallel watch:script watch:style"
  },
  "dependencies": {
    "axios": "^0.15.0",
    "classnames": "^2.2.0",
    "cookie-machine": "^0.1.1",
    "history": "^2.1.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-helmet": "^2.1.1",
    "react-redux": "^4.0.0",
    "react-resolver": "^2.0.5",
    "react-router": "^2.8.1",
    "react-router-scroll": "^0.3.2",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "buble": "^0.13.0",
    "buble-loader": "^0.2.2",
    "cssnano": "^3.5.2",
    "dotenv": "^1.2.0",
    "dotenv-autoload": "^0.1.3",
    "node-sass": "^3.4.2",
    "npm-run-all": "^1.4.0",
    "postcss": "^5.0.19",
    "postcss-cli": "^2.5.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "stringify-object-values": "^0.1.0",
    "webpack": "2.1.0-beta.25"
  }
}
